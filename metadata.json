{
  "name": "shoekstra-apticron",
  "version": "0.1.1",
  "author": "shoekstra",
  "summary": "Puppet module for apticron",
  "license": "Apache-2.0",
  "source": "https://github.com/shoekstra/puppet-apticron",
  "project_page": "https://github.com/shoekstra/puppet-apticron",
  "issues_url": "https://github.com/shoekstra/puppet-apticron/issues",
  "description": "Puppet module to install and configure Apticron, a simple script which sends daily emails about pending package updates on Debian based systems.",
  "tags": [
    "apt",
    "apticron"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.4.0 <= 4.5.0"
    }
  ]
}
